# Export resolution (higher values = higher quality but larger file size)
# Default is 2 if not specified
VITE_EXPORT_PIXEL_RATIO=4

# Designable Area Configuration
# Default position and size of the designable area
VITE_DESIGNABLE_AREA_X=150
VITE_DESIGNABLE_AREA_Y=150
VITE_DESIGNABLE_AREA_WIDTH=300
VITE_DESIGNABLE_AREA_HEIGHT=300

# Whether the designable area is visible by default
# Set to false to hide it initially
VITE_DESIGNABLE_AREA_VISIBLE=false
